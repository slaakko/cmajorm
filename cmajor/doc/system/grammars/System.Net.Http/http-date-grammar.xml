<grammar source="HttpDateParser" title="HTTP Date Grammar">
    <rule source="HttpDate" name="http-date"/>
    <rule source="RFC1123Date" name="rfc-1123-date"/>
    <rule source="RFC850Date" name="rfc-850-date"/>
    <rule source="AscTimeDate" name="asc-time-date"/>
    <rule source="date1" name="date1"/>
    <rule source="date2" name="date2"/>
    <rule source="date3" name="date3"/>
    <rule source="Year" name="year"/>
    <rule source="Year2" name="year2"/>
    <rule source="Day" name="day"/>
    <rule source="Day1" name="day1"/>
    <rule source="wkday" name="wkday"/>
    <rule source="weekday" name="weekday"/>
    <rule source="month" name="month"/>
    <rule source="time" name="time"/>
    <rule source="Hour" name="hour"/>
    <rule source="Min" name="min"/>
    <rule source="Sec" name="sec"/>
    <rule source="deltaSeconds" name="delta-seconds"/>
    <rule source="SP" name="sp"/>
    <rule source="DIGIT" name="digit"/>
</grammar>
