<grammar source="ParserFileParser" title="Parser File Grammar">
    <rule source="ParserFile" name="parser-file"/>
    <rule source="Declaration" name="declaration"/>
    <rule source="UsingDirective" name="using-directive"/>
    <rule source="UsingAliasDirective" name="using-alias-directive"/>
    <rule source="UsingNamespaceDirective" name="using-namespace-directive"/>
    <rule source="ParserDeclaration" name="parser-declaration"/>
    <rule source="Statement" name="statement"/>
    <rule source="MainStatement" name="main-statement"/>
    <rule source="UsingStatement" name="using-statement"/>
    <rule source="UsingQualifiedId" name="using-qualified-id"/>
    <rule source="LexerStatement" name="lexer-statement"/>
    <rule source="RuleInfoStatement" name="rule-info-statement"/>
    <rule source="RuleInfo" name="rule-info"/>
    <rule source="RuleStatement" name="rule-statement"/>
    <rule source="ParametersAndVariables" name="parameters-and-variables"/>
    <rule source="ParamOrVariable" name="param-or-variable"/>
    <rule source="ReturnType" name="return-type"/>
    <rule source="RuleBody" name="rule-body"/>
    <rule source="Alternative" name="alternative"/>
    <rule source="Sequence" name="sequence"/>
    <rule source="Difference" name="difference"/>
    <rule source="List" name="list"/>
    <rule source="Postfix" name="postfix"/>
    <rule source="Primary" name="primary"/>
    <rule source="RuleCall" name="rule-call"/>
    <rule source="Nonterminal" name="nonterminal"/>
    <rule source="Primitive" name="primitive"/>
    <rule source="Grouping" name="grouping"/>
</grammar>
