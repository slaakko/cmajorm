<grammar kind="lexical" source="ProjectFileLexer" title="Lexical Project File Grammar">
    <rule source="ws" name="white-space"/>
    <rule source="newline" name="newline"/>
    <rule source="linecomment" name="line-comment"/>
    <rule source="blockcomment" name="block-comment"/>
    <rule source="comment" name="comment"/>
    <rule source="separators" name="separators"/>
    <rule source="id" name="id"/>
    <rule source="filepath" name="file-path"/>
</grammar>
