<grammar source="AssemblyParser" title="Assembly Grammar">
    <rule source="AssemblyFile" name="assembly-file"/>
    <rule source="AssemblyLine" name="assembly-line"/>
    <rule source="CommentLine" name="comment-line"/>
    <rule source="Comment" name="comment"/>
    <rule source="EmptyLine" name="empty-line"/>
    <rule source="ModeLine" name="mode-line"/>
    <rule source="InstructionLine" name="instruction-line"/>
    <rule source="Instruction" name="instruction"/>
    <rule source="Label" name="label"/>
    <rule source="SplitLine" name="split-line"/>
    <rule source="OpCode" name="opcode"/>
    <rule source="Operands" name="operands"/>
    <rule source="Expression" name="expression"/>
    <rule source="WeakOperator" name="weak-operator"/>
    <rule source="Term" name="term"/>
    <rule source="StrongOperator" name="strong-operator"/>
    <rule source="PrimaryExpression" name="primary-expression"/>
    <rule source="UnaryOperator" name="unary-operator"/>
    <rule source="LocalSymbol" name="local-symbol"/>
    <rule source="Constant" name="constant"/>
    <rule source="Symbol" name="symbol"/>
    <rule source="At" name="at"/>
</grammar>
