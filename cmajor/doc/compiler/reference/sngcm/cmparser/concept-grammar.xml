<?xml version="1.0" encoding="utf-8"?>
<grammar source="ConceptParser" title="Concept Grammar">
  <rule source="Concept" name="concept"/>
  <rule source="Refinement" name="refinement"/>
  <rule source="ConceptBody" name="concept-body"/>
  <rule source="ConceptBodyConstraint" name="concept-body-constraint"/>
  <rule source="TypeNameConstraint" name="type-name-constraint"/>
  <rule source="SignatureConstraint" name="signature-constraint"/>
  <rule source="ConstructorConstraint" name="constructor-constraint"/>
  <rule source="DestructorConstraint" name="destructor-constraint"/>
  <rule source="MemberFunctionConstraint" name="member-function-constraint"/>
  <rule source="FunctionConstraint" name="function-constraint"/>
  <rule source="EmbeddedConstraint" name="embedded-constraint"/>
  <rule source="WhereConstraint" name="where-constraint"/>
  <rule source="ConstraintExpr" name="constraint-expr"/>
  <rule source="DisjunctiveConstraintExpr" name="disjunctive-constraint-expr"/>
  <rule source="ConjunctiveConstraintExpr" name="conjunctive-constraint-expr"/>
  <rule source="PrimaryConstraintExpr" name="primary-constraint-expr"/>
  <rule source="AtomicConstraintExpr" name="atomic-constraint-expr"/>
  <rule source="PredicateConstraint" name="predicate-constraint-expr"/>
  <rule source="IsConstraint" name="is-constraint"/>
  <rule source="ConceptOrTypeName" name="concept-or-type-name"/>
  <rule source="MultiParamConstraint" name="multiparam-constraint"/>
  <rule source="Axiom" name="axiom"/>
  <rule source="AxiomBody" name="axiom-body"/>
  <rule source="AxiomStatement" name="axiom-statement"/>
</grammar>
