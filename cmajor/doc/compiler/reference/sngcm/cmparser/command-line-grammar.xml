<grammar source="CommandLineParser" title="Command Line Grammar">
  <rule source="CommandLine" name="command-line"/>
  <rule source="Spaces" name="spaces"/>
  <rule source="Argument" name="argument"/>
  <rule source="ArgElement" name="arg-element"/>
  <rule source="OddBackslashesAndLiteralQuotationMark" name="odd-backslahses-and-literal-quotation-mark"/>
  <rule source="EvenBackslashesAndQuotationMark" name="even-backslahses-and-literal-quotation-mark"/>
  <rule source="StringChar" name="string-char"/>
</grammar>
