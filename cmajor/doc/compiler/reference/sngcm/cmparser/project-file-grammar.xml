<?xml version="1.0" encoding="utf-8"?>
<grammar source="ProjectFileParser" title="Project File Grammar">
  <rule source="ProjectFile" name="project-file"/>
  <rule source="QualifiedId" name="qualified-id"/>
  <rule source="Declaration" name="declaration"/>
  <rule source="ReferenceDeclaration" name="reference-declaration"/>
  <rule source="SourceFileDeclaration" name="source-file-declaration"/>
  <rule source="TextFileDeclaration" name="text-file-declaration"/>
  <rule source="TargetDeclaration" name="target-declaration"/>
  <rule source="Target" name="target"/>
</grammar>
