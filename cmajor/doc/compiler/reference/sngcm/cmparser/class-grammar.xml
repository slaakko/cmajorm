<?xml version="1.0" encoding="utf-8"?>
<grammar source="ClassParser" title="Class Grammar">
  <rule source="Class" name="class"/>
  <rule source="InheritanceAndInterfaces" name="inheritance-and-interfaces"/>
  <rule source="BaseClassOrInterface" name="base-class-or-interface"/>
  <rule source="ClassContent" name="class-content"/>
  <rule source="ClassMember" name="class-member"/>
  <rule source="StaticConstructor" name="static-constructor"/>
  <rule source="Constructor" name="constructor"/>
  <rule source="Destructor" name="destructor"/>
  <rule source="Initializer" name="initializer"/>
  <rule source="MemberFunction" name="member-function"/>
  <rule source="ConversionFunction" name="conversion-function"/>
  <rule source="MemberVariable" name="member-variable"/>
</grammar>
