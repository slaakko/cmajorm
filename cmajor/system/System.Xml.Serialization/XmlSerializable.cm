using System;

namespace System.Xml.Serialization
{
    public interface XmlSerializable
    {
        const Uuid& XmlId();
        void* XmlThis();
        XmlBundle* XmlOwner();
        void SetXmlOwner(XmlBundle* owner);
        string XmlClassName();
        void* XmlCreate();
        void* XmlClone();
    }
}


