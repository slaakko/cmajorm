using System;

public nothrow double Pow(double x, int exponent)
{
    return RtPow(x, exponent);
}

public nothrow double Sqrt(double x)
{
    return RtSqrt(x);
}

public nothrow int Log10(int n)
{
    int log10 = 1;
    int m = n / 10;
    while (m > 0)
    {
        ++log10;
        m = m / 10;
    }
    return log10;
}
