using System;

namespace System.Net.Http
{
    public uchar ParseHex(uchar c1, uchar c2)
    {
        string s1(cast<char>(c1));
        byte b0 = ParseHexByte(s1);
        string s2(cast<char>(c1));
        byte b1 = ParseHexByte(s2);
        return cast<uchar>(16u * cast<uint>(b0) + cast<uint>(b1));
    }
}
