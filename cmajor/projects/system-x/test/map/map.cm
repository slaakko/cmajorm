using System;
using System.Collections;

string MakeString()
{
	string s;
	uint n = 16u * (RandomNumber(16u) + 1u);
	for (uint i = 0u; i < n; ++i)
	{
		byte x = cast<byte>(i);
		s.Append(ToHexString(x));
	}
	return s;
}

void main()
{
	Map<int, string> m;
	for (int i = 0; i < 10; ++i)
	{
		m[i] = MakeString();
	}
	for (int i = 0; i < 10; ++i)
	{
		Map<int, string>.ConstIterator it = m.CFind(i);
		if (it != m.CEnd())
		{
			string s = ToString(i) + ": " + it->second + "\n";
			write(1, s.Chars(), s.Length());
		}
	}
}
