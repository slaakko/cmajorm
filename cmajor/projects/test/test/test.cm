using System;
using System.Collections;

void main()
{
    Map<string, string> map;
    map["abc"] = "abc";
    map["bar"] = "bar";
    map["foo"] = "foo";
    Map<string, string>.ConstIterator it = map.CLowerBound("abc");
    while (it != map.CEnd())
    {
        const string& s = it->second;
        Console.Out() << s << endl();
        ++it;
    }
}
