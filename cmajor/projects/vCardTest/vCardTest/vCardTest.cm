using System;
using vCard;

namespace vCard
{
    public class vCardClass
    {
        public string version;
    }
}

int main()
{
    try
    {
        string str = "BEGIN:VCARD\nVERSION:4.0\nEND:VCARD\n";
        ustring content = ToUtf32(str);
        TrivialLexer lexer(content, "", 0);
        UniquePtr<vCardClass> vc = vCardTestParser.Parse(lexer);
    }
    catch (const Exception& ex)
    {
        Console.Error() << ex.Message() << endl();
        return 1;
    }
    return 0;
}
