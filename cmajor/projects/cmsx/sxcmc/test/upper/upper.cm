using System;

int main(int argc, const char** argv)
{
    try
    {
        Console.Out() << "line> ";
        string line = Console.In().ReadLine();
        string upper = ToUpper(line);
        Console.Out() << upper << endl();
    }
    catch (const Exception& ex)
    {
        Console.Error() << ex.Message() << endl();
        return 1;
    }
    return 0;
}
