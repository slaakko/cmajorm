using System;

class Foo
{
    public default virtual ~Foo();
    public virtual void foo()
    {
        Console.WriteLine("foo");
    }
}

class Bar : Foo
{
    public override void foo()
    {
        Console.WriteLine("bar");
    }
}

class Baz
{
    public default virtual ~Baz();
}

void main()
{
    Foo f;
    Bar b;
    Foo* foo = &f;
    foo->foo();
    foo = &b;
    foo->foo();
    Baz baz;
}
