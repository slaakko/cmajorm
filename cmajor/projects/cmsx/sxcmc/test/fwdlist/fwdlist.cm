using System;
using System.Collections;

int main()
{
    try
    {
        ForwardList<int> f;
        f.InsertFront(1);
        f.InsertFront(2);
        f.InsertFront(3);
        bool first = true;
        for (int x : f)
        {
            if (first)
            {
                first = false;
            }
            else
            {
                Console.Out() << ", ";
            }
            Console.Out() << x;
        }
        f.RemoveFront();
        f.RemoveFront();
        f.RemoveFront();
        Console.Out() << endl();
    }
    catch (const Exception& ex)
    {
        Console.Error() << ex.ToString() << endl();
        return 1;
    }
    return 0;
}