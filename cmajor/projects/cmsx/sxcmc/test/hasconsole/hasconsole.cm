using System;
using System.Os;
using System.IO;

int main()
{
    try
    {
        bool hasConsole = IsConsole(stdout);
        if (hasConsole)
        {
            Console.Out() << "has console" << endl();
        }
        else
        {
            Console.Out() << "no console" << endl();
        }
    }
    catch (const Exception& ex)
    {
        Console.Out() << ex.Message() << endl();
        return 1;
    }
    return 0;
}