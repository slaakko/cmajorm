            NfaState* start = lexerContext.MakeNfaState();
            masterNfa.SetStart(start);
            for (const UniquePtr<LexerStatement>& statement : statements)
            {
                start->AddEdge(NfaEdge(lexerContext.MakeEpsilon(), statement->GetNfa().Start()));
