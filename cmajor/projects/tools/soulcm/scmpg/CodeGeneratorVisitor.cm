    public void GenerateRuleFile(Domain& domain, bool verbose)
    {
        StreamWriter writer = File.CreateText(domain.RuleFilePath());
        CodeFormatter formatter(writer);
        formatter.WriteLine("using System;");
        formatter.WriteLine("using System.Collections;");
        formatter.WriteLine();
        formatter.WriteLine("public class RuleNames");
        formatter.WriteLine("{");
        formatter.IncIndent();
        formatter.WriteLine("static RuleNames() : instance(new RuleNames())");
        formatter.WriteLine("{");
        formatter.WriteLine("}");
        formatter.WriteLine("public static RuleNames& Instance()");
        formatter.WriteLine("{");
        formatter.IncIndent();
        formatter.WriteLine("return *instance;");
        formatter.DecIndent();
        formatter.WriteLine("}");
        formatter.WriteLine("public nothrow RuleNames()");
        formatter.WriteLine("{");
        formatter.IncIndent();
        for (RuleParser* rule : domain.Rules())
        {
            formatter.WriteLine("ruleNames.Add(\"" + ToUtf8(rule->Parent()->Name()) + "." + ToUtf8(rule->Name()) + "\");");
        }
        formatter.DecIndent();
        formatter.WriteLine("}");
        formatter.WriteLine("public nothrow List<string>* Get() const");
        formatter.WriteLine("{");
        formatter.IncIndent();
        formatter.WriteLine("return &ruleNames;");
        formatter.DecIndent();
        formatter.WriteLine("}");
        formatter.WriteLine("private static UniquePtr<RuleNames> instance;");
        formatter.WriteLine("private List<string> ruleNames;");
        formatter.DecIndent();
        formatter.WriteLine("}");
        formatter.WriteLine();
        formatter.WriteLine("public nothrow List<string>* GetRuleNameList()");
        formatter.WriteLine("{");
        formatter.IncIndent();
        formatter.WriteLine("return RuleNames.Instance().Get();");
        formatter.DecIndent();
        formatter.WriteLine("}");
        formatter.WriteLine();
        if (verbose)
        {
            Console.Out() << "==> " << domain.RuleFilePath() << endl();
        }
    }
    public string ParserGeneratorVersionStr()
    {
        return "5.0.0";
    }
