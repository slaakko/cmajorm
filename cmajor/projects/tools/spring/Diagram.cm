            {
                cloneMap[fromClass->operations.Get(i)] = operations.Get(i);
            }
            int nf = fromClass->fields.Count();
            for (int i = 0; i < nf; ++i)
            {
                cloneMap[fromClass->fields.Get(i)] = fields.Get(i);
            }
