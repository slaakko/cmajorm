        public nothrow int GetIndexOfElement(DiagramElement* element)
        {
            int n = elements.Count();
            for (int i = 0; i < n; ++i)
            {
                if (elements.Get(i) == element)
                {
                    return i;
                }
            }
            return -1;
        }
