using System;
using System.Collections;

public void foo(List<int>& x)
{
    int n = 10;
    for (int i = 0; i < n; ++i)
    {
        x.Add(i);
    }
}

public void bar()
{
    List<int> x;
    foo(x);
    print(x);
}

void print(List<int>& x)
{
    for (int i : x)
    {
        Console.WriteLine(i);
    }
}

void main()
{
    bar();
}
