using System;

const int FOO = -2;

void main()
{
	int x = -2;
	if (x == -2)
	{
		Console.Out() << x << " == " << -2 << endl();
	}
	else 
	{
		Console.Out() << x << " != " << FOO << endl();
	}	
	if (x == FOO)
	{
		Console.Out() << x << " == " << FOO << endl();
	}
	else 
	{
		Console.Out() << x << " != " << FOO << endl();
	}	
}
