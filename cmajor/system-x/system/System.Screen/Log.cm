using System;
using System.IO;

namespace System.Screen
{
	public class Log
	{
		static Log() : instance(new Log())
		{
		}
		private Log()
		{
		}
		public static Log& Instance()
		{
			return *instance;
		}
		private static UniquePtr<Log> instance;
	}
}
