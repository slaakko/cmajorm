using System;

int main(int argc, const char** argv)
{
	try
	{
		Install();
		ShellEnvironment environment;
		SetupShellEnvironment(environment);
		Run(environment);
	}
	catch (const Exception& ex)
	{
		Console.Error() << ex.ToString() << endl();
		return 1;
	}
	return 1;
}
