using System;
using System.Collections;
using System.RegularExpressions;

int main()
{
	try
	{
		if (FilePatternMatch(u"abc.cm", u"*.cm"))
		{
			Console.WriteLine("abc.cm matches");
		}
		else
		{
			Console.WriteLine("abc.cm does not match");
		}
		if (FilePatternMatch(u"abc.c", u"*.cm"))
		{
			Console.WriteLine("abc.c matches");
		}
		else
		{
			Console.WriteLine("abc.c does not match");
		}
	}
	catch (const Exception& ex)
	{
		Console.Error() << ex.ToString() << endl();
		return 1;
	}
	return 0;
}
