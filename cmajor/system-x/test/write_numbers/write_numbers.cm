using System;

void print(int i)
{
	Console.Write(i);
}

void print_comma()
{
	Console.Write(", ");
}

void print_line()
{
	Console.WriteLine();
}

void write_numbers()
{
	for (int i = 0; i < 3; ++i)
	{
		if (i > 0)
		{
			print_comma();
		}
		print(i);
	}
	print_line();
}

void main()
{
	write_numbers();
}
