using System;
using System.Os;

int main()
{
	try
	{
		int fd = Create("foo", 0);
		string s = "foo\n";
		Write(fd, s.Chars(), s.Length());
		Seek(fd, 4096, Origin.seekSet);
		Write(fd, "x", 1);
		long pos = Tell(fd);
		Console.WriteLine(pos);
		Close(fd);
	}
	catch (const Exception& ex)
	{
		Console.Error() << ex.ToString() << endl();
		return 1;
	}
	return 0;
}
