<ProjectSchemaDefinitions
    xmlns="http://schemas.microsoft.com/build/2009/properties">

    <ContentType
        Name="Text"
        DisplayName="Text file"
        ItemType="None">
    </ContentType>

    <ContentType
        Name="CmSourceFile"
        DisplayName="Cmajor source file"
        ItemType="CmCompile">
    </ContentType>

    <ItemType Name="CmCompile" DisplayName="Cmajor source file"/>
    <ItemType Name="None" DisplayName="Does not participate in build"/>

    <FileExtension Name=".txt" ContentType="Text"/>
    <FileExtension Name=".cm" ContentType="CmSourceFile" />
</ProjectSchemaDefinitions>
